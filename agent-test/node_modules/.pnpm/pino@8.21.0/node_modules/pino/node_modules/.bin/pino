#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Noah/OneDrive/Documents/Code/MultimodalVoice/voice-assistant-frontend/agent-test/node_modules/.pnpm/pino@8.21.0/node_modules/pino/node_modules:/mnt/c/Users/Noah/OneDrive/Documents/Code/MultimodalVoice/voice-assistant-frontend/agent-test/node_modules/.pnpm/pino@8.21.0/node_modules:/mnt/c/Users/Noah/OneDrive/Documents/Code/MultimodalVoice/voice-assistant-frontend/agent-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Noah/OneDrive/Documents/Code/MultimodalVoice/voice-assistant-frontend/agent-test/node_modules/.pnpm/pino@8.21.0/node_modules/pino/node_modules:/mnt/c/Users/Noah/OneDrive/Documents/Code/MultimodalVoice/voice-assistant-frontend/agent-test/node_modules/.pnpm/pino@8.21.0/node_modules:/mnt/c/Users/Noah/OneDrive/Documents/Code/MultimodalVoice/voice-assistant-frontend/agent-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
